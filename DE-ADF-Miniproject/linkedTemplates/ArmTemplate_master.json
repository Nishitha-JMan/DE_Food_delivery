{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DE-ADF-Miniproject"
		},
		"SnowflakeDE_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SnowflakeDE'"
		},
		"SqlServerIR_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerIR'"
		},
		"SnowflakeDE_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Food_Delivery"
		},
		"SnowflakeDE_properties_typeProperties_role": {
			"type": "string",
			"defaultValue": "ACCOUNTADMIN"
		},
		"SqlServerIR_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "JM-DKT-041"
		},
		"SqlServerIR_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Food_Delivery"
		},
		"SqlServerIR_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"linkedServicefrostaging_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'linkedServicefrostaging'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DE-ADF-Miniproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SnowflakeDE_password": {
						"value": "[parameters('SnowflakeDE_password')]"
					},
					"SqlServerIR_password": {
						"value": "[parameters('SqlServerIR_password')]"
					},
					"linkedServicefrostaging_sasUri": {
						"value": "[parameters('linkedServicefrostaging_sasUri')]"
					},
					"SnowflakeDE_properties_typeProperties_database": {
						"value": "[parameters('SnowflakeDE_properties_typeProperties_database')]"
					},
					"SnowflakeDE_properties_typeProperties_role": {
						"value": "[parameters('SnowflakeDE_properties_typeProperties_role')]"
					},
					"SqlServerIR_properties_typeProperties_server": {
						"value": "[parameters('SqlServerIR_properties_typeProperties_server')]"
					},
					"SqlServerIR_properties_typeProperties_database": {
						"value": "[parameters('SqlServerIR_properties_typeProperties_database')]"
					},
					"SqlServerIR_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerIR_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}